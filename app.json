{
  "expo": {
    "name": "MyCademy",
    "slug": "MyCademy",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/icons/splash-screen.png",
      "resizeMode": "fill",
      "backgroundColor": "#212529"
    },
    "scheme": "mycademy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alyyy07.MyCademy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/logo.png",
        "backgroundColor": "#212529"
      },
      "package": "com.alyyy07.MyCademy",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#212529"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.266720961272-c84k373c5ls49vccuflee0vsp74j8epq"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b204edb2-0073-411e-a415-08f164a42754"
      }
    },
    "owner": "alyyy07"
  }
}
