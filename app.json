{
  "expo": {
    "name": "MyCademy",
    "slug": "MyCademy",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/icons/splash-screen.png",
      "resizeMode": "fill",
      "backgroundColor": "#212529"
    },
    "scheme": "mycademy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#212529"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#212529"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
